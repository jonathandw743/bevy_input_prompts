#![feature(prelude_import)]
#[prelude_import]
use std::prelude::rust_2024::*;
#[macro_use]
extern crate std;
use std::{env, fs, path::{Path, PathBuf}};
use bevy_input::keyboard::KeyCode;
use fs_extra::dir::CopyOptions;
pub mod directory_representation {
    pub mod _test {
        pub const PATH: &'static str = "assets/test";
        pub enum _MX_0 {
            _b,
            _c,
        }
        pub enum _MX_1 {
            _a,
        }
        pub fn file(
            possible_file: (Option<_MX_0>, Option<_MX_1>),
        ) -> Option<&'static str> {
            match possible_file {
                (Some(_MX_0::_c), None) => Some("assets/test/c.png"),
                (Some(_MX_0::_b), Some(_MX_1::_a)) => Some("assets/test/a_b.png"),
                _ => None,
            }
        }
    }
}
